Content.find(".non-insider-content").show();$insiderContent.find(".insider-content").hide();}$insiderContent.show();}function parseJSON(data,element){if(espn.core.is(data[element],"String")){var parsed=JSON.parse(data[element]);data[element]=parsed;}return data;}function isSoccer(sportId){var isSoccerSport=false;if((sportId>=600&&sportId<800)||(sportId>3900&&sportId<3999)){isSoccerSport=true;}return isSoccerSport;}function cleanupJSON(data){$.each(["mySports","myTeams","myPlayers","myColumnists","myModules"],function(i,key){data=parseJSON(data,key);});return data;}function imageErrorCheck(images,replaceUrl){$(images).each(function(){$(this).attr("src",replaceUrl);});}function mergeSportsModules(mySports,myModules){var myItems=[],modulePosition=0,nada;function isMenu(item){var isMenuSection=false,shownSoccer=false,sportRoot="";if(typeof(item.text)!=="undefined"&&item.text!==null){sportRoot=item.text.toLowerCase();sportRoot=sportRoot.replace(" ","-");}if(item.kind==="sports"&&isSoccer(item.id)){if(!shownSoccer){sportRoot="soccer";}else{isMenuSection=false;}}var $thisMenu=$("#menu-"+sportRoot);if($thisMenu.length!==0){isMenuSection=true;}return isMenuSection;}if(typeof(mySports)!=="undefined"&&mySports!==null&&typeof(myModules)!=="undefined"&&myModules!==null){var i;$.each(mySports,function(i,item){item.kind="sports";if(isMenu(item)){nada=myItems.push(item);}});modulePosition=7-myModules.length;$.each(myModules,function(i,item){item.kind="modules";if(isMenu(item)){nada=myItems.splice(modulePosition,0,item);modulePosition++;}});}return myItems;}"espn.core.nav".namespace();espn.core.nav.imageErrorCheck=imageErrorCheck;var sportIdsToRoots={28:"nfl",10:"mlb",46:"nba",90:"nhl",23:"ncf",41:"ncb",54:"ncw"};var sportMatrix={23:"NCAAF",41:"NCAAB",54:"NCAAW"};function initMyColumnists($div){espn.p13n.get({success:function(data){data=cleanupJSON(data);var itemCount=0,maxItemCount=3,html="",columnistData;var columnistFirstName={Ja:"J.A.",Dj:"DJ",Lz:"LZ",Ej:"E.J.",Amy:"Amy K.",Lean